<?xml version="1.0" ?>

<BIF VERSION="0.3">
<NETWORK>

<!-- Variables -->
<VARIABLE TYPE="nature">
	<NAME>CL1</NAME>
	<PROPERTY>L1 component</PROPERTY>
	<OUTCOME>on</OUTCOME>
	<OUTCOME>off</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="nature">
	<NAME>HL1</NAME>
	<PROPERTY>L1 health</PROPERTY>
	<OUTCOME>ok</OUTCOME>
	<OUTCOME>broken</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="nature">
	<NAME>CS1</NAME>
	<PROPERTY>S1 component</PROPERTY>
	<OUTCOME>on</OUTCOME>
	<OUTCOME>off</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="nature">
	<NAME>HS1</NAME>
	<PROPERTY>S1 health</PROPERTY>
	<OUTCOME>ok</OUTCOME>
	<OUTCOME>broken</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="decision">
	<NAME>DT1</NAME>
	<PROPERTY>Decide T1</PROPERTY>
	<OUTCOME>yes</OUTCOME>
	<OUTCOME>no</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="nature">
	<NAME>RT1</NAME>
	<PROPERTY>Result T1</PROPERTY>
	<OUTCOME>not done</OUTCOME>
	<OUTCOME>OK</OUTCOME>
	<OUTCOME>broken</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="utility">
	<NAME>UT1</NAME>
	<PROPERTY>R(L1H, DT1)</PROPERTY>
	<OUTCOME>0</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="decision">
	<NAME>DT2</NAME>
	<PROPERTY>Decide T2</PROPERTY>
	<OUTCOME>yes</OUTCOME>
	<OUTCOME>no</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="nature">
	<NAME>RT2</NAME>
	<PROPERTY>Result T2</PROPERTY>
	<OUTCOME>not done</OUTCOME>
	<OUTCOME>OK</OUTCOME>
	<OUTCOME>broken</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="utility">
	<NAME>UT2</NAME>
	<PROPERTY>R(L1H, DT2)</PROPERTY>
	<OUTCOME>0</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="decision">
	<NAME>DS1T1</NAME>
	<PROPERTY>Decide S1 T1</PROPERTY>
	<OUTCOME>yes</OUTCOME>
	<OUTCOME>no</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="nature">
	<NAME>RS1T1</NAME>
	<PROPERTY>Result S1 T1</PROPERTY>
	<OUTCOME>not done</OUTCOME>
	<OUTCOME>OK</OUTCOME>
	<OUTCOME>broken</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="utility">
	<NAME>S1UT1</NAME>
	<PROPERTY>R(S1H, S1DT1)</PROPERTY>
	<OUTCOME>0</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="decision">
	<NAME>DRL1</NAME>
	<PROPERTY>L1 replacement decision</PROPERTY>
	<OUTCOME>yes</OUTCOME>
	<OUTCOME>no</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="utility">
	<NAME>URL1</NAME>
	<PROPERTY>R(RT2, DRL1)</PROPERTY>
	<OUTCOME>0</OUTCOME>
</VARIABLE>

<!-- Probability distributions -->
<DEFINITION>
	<FOR>CL1</FOR>
	<GIVEN>CS1</GIVEN>
	<GIVEN>HL1</GIVEN>
	<TABLE>0.95 0.05 0.1 0.9 0.05 0.95 0.01 0.99 </TABLE>
</DEFINITION>
<DEFINITION>
	<FOR>HL1</FOR>
	<TABLE>0.7 0.3 </TABLE>
</DEFINITION>
<DEFINITION>
	<FOR>CS1</FOR>
	<GIVEN>HS1</GIVEN>
	<TABLE>0.7 0.3 0.5 0.5 </TABLE>
</DEFINITION>
<DEFINITION>
	<FOR>HS1</FOR>
	<TABLE>0.9 0.1 </TABLE>
</DEFINITION>
<DEFINITION>
	<FOR>RT1</FOR>
	<GIVEN>DT1</GIVEN>
	<TABLE>0 0.8 0.2 0.95 0.05 0 </TABLE>
</DEFINITION>
<DEFINITION>
	<FOR>UT1</FOR>
	<GIVEN>DT1</GIVEN>
	<GIVEN>HL1</GIVEN>
	<TABLE>-100 -1 -10 -30 </TABLE>
</DEFINITION>
<DEFINITION>
	<FOR>RT2</FOR>
	<GIVEN>DT2</GIVEN>
	<TABLE>0 0.2 0.8 1 0 0 </TABLE>
</DEFINITION>
<DEFINITION>
	<FOR>UT2</FOR>
	<GIVEN>RT1</GIVEN>
	<GIVEN>DT2</GIVEN>
	<TABLE>-100 -10 -100 -10 0 -20 </TABLE>
</DEFINITION>
<DEFINITION>
	<FOR>DS1T1</FOR>
	<GIVEN>HS1</GIVEN>
</DEFINITION>
<DEFINITION>
	<FOR>RS1T1</FOR>
	<GIVEN>DS1T1</GIVEN>
	<TABLE>0 0.4 0.6 1 0 0 </TABLE>
</DEFINITION>
<DEFINITION>
	<FOR>S1UT1</FOR>
	<GIVEN>DS1T1</GIVEN>
	<GIVEN>HS1</GIVEN>
	<TABLE>-50 -1 -10 -30 </TABLE>
</DEFINITION>
<DEFINITION>
	<FOR>URL1</FOR>
	<GIVEN>DRL1</GIVEN>
	<GIVEN>RT2</GIVEN>
	<TABLE>-200 -100 -300 -100 0 -100 </TABLE>
</DEFINITION>

</NETWORK>
</BIF>
