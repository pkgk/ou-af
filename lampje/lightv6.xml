<?xml version="1.0" ?>

<BIF VERSION="0.3">
<NETWORK>

<!-- Variables -->
<VARIABLE TYPE="nature">
	<NAME>CL1</NAME>
	<PROPERTY>L1 component</PROPERTY>
	<OUTCOME>on</OUTCOME>
	<OUTCOME>off</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="nature">
	<NAME>HL1</NAME>
	<PROPERTY>L1 health</PROPERTY>
	<OUTCOME>ok</OUTCOME>
	<OUTCOME>broken</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="decision">
	<NAME>DL1T1</NAME>
	<PROPERTY>Decide T1</PROPERTY>
	<OUTCOME>yes</OUTCOME>
	<OUTCOME>no</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="nature">
	<NAME>RL1T1</NAME>
	<PROPERTY>Result T1</PROPERTY>
	<OUTCOME>not done</OUTCOME>
	<OUTCOME>ok</OUTCOME>
	<OUTCOME>broken</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="utility">
	<NAME>UL1T1</NAME>
	<PROPERTY>R(L1H, DT1)</PROPERTY>
	<OUTCOME>0</OUTCOME>
</VARIABLE>

<!-- Probability distributions -->
<DEFINITION>
	<FOR>CL1</FOR><!--CL1 | HL1,-->
	<GIVEN>HL1</GIVEN>
	<TABLE>0.1 0.9 0.05 0.95 </TABLE>
</DEFINITION>
<DEFINITION>
	<FOR>HL1</FOR><!--HL1 | -->
	<TABLE>0.7 0.3 </TABLE>
</DEFINITION>
<DEFINITION>
	<FOR>RL1T1</FOR><!--RL1T1 | HL1,-->
	<GIVEN>HL1</GIVEN>
	<TABLE>0 0 0 0 0 0 </TABLE>
</DEFINITION>
<DEFINITION>
	<FOR>UL1T1</FOR><!--UL1T1 | HL1,DL1T1,-->
	<GIVEN>HL1</GIVEN>
	<GIVEN>DL1T1</GIVEN>
	<TABLE>1 2 3 0 </TABLE>
</DEFINITION>

</NETWORK>
</BIF>
