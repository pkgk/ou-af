<?xml version="1.0" ?>

<BIF VERSION="0.3">
<NETWORK>

<!-- Variables -->
<VARIABLE TYPE="nature">
	<NAME>CL1</NAME>
	<PROPERTY>L1 component</PROPERTY>
	<OUTCOME>on</OUTCOME>
	<OUTCOME>off</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="nature">
	<NAME>HL1</NAME>
	<PROPERTY>L1 health</PROPERTY>
	<OUTCOME>ok</OUTCOME>
	<OUTCOME>broken</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="decision">
	<NAME>DT1</NAME>
	<PROPERTY>Decide T1</PROPERTY>
	<OUTCOME>yes</OUTCOME>
	<OUTCOME>no</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="nature">
	<NAME>RT1</NAME>
	<PROPERTY>Result T1</PROPERTY>
	<OUTCOME>not done</OUTCOME>
	<OUTCOME>ok</OUTCOME>
	<OUTCOME>broken</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="utility">
	<NAME>UT1</NAME>
	<PROPERTY>R(L1H, DT1)</PROPERTY>
	<OUTCOME>0</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="decision">
	<NAME>DT2</NAME>
	<PROPERTY>Decide T2</PROPERTY>
	<OUTCOME>yes</OUTCOME>
	<OUTCOME>no</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="nature">
	<NAME>RT2</NAME>
	<PROPERTY>Result T2</PROPERTY>
	<OUTCOME>not done</OUTCOME>
	<OUTCOME>ok</OUTCOME>
	<OUTCOME>broken</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="utility">
	<NAME>UT2</NAME>
	<PROPERTY>R(RT1, DT2)</PROPERTY>
	<OUTCOME>0</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="decision">
	<NAME>DRL1</NAME>
	<PROPERTY>replace Light 1 T2</PROPERTY>
	<OUTCOME>replace</OUTCOME>
	<OUTCOME>keep</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="utility">
	<NAME>URL1</NAME>
	<PROPERTY>R(RT2, DRL1)</PROPERTY>
	<OUTCOME>0</OUTCOME>
</VARIABLE>

<!-- Probability distributions -->
<DEFINITION>
	<FOR>CL1</FOR>
	<GIVEN>HL1</GIVEN>
	<TABLE>0.95 0.05 0.1 0.9 </TABLE>
</DEFINITION>
<DEFINITION>
	<FOR>HL1</FOR>
	<TABLE>0.7 0.3 </TABLE>
</DEFINITION>
<DEFINITION>
	<FOR>RT1</FOR>
	<GIVEN>HL1</GIVEN>
	<TABLE>0.95 0.05 0 0.4 0.1 0.5 </TABLE>
</DEFINITION>
<DEFINITION>
	<FOR>UT1</FOR>
	<GIVEN>DT1</GIVEN>
	<GIVEN>HL1</GIVEN>
	<TABLE>1 2 3 0 </TABLE>
</DEFINITION>
<DEFINITION>
	<FOR>RT2</FOR>
	<GIVEN>RT1</GIVEN>
	<TABLE>0.95 0.05 0 0.05 0.9 0.05 0.4 0.1 0.5 </TABLE>
</DEFINITION>
<DEFINITION>
	<FOR>UT2</FOR>
	<GIVEN>DT2</GIVEN>
	<GIVEN>RT1</GIVEN>
	<TABLE>2 4 0 3 5 2 </TABLE>
</DEFINITION>
<DEFINITION>
	<FOR>URL1</FOR>
	<GIVEN>RT2</GIVEN>
	<GIVEN>DRL1</GIVEN>
	<TABLE>2 4 0 3 4 4 </TABLE>
</DEFINITION>

</NETWORK>
</BIF>
