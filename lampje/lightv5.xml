<?xml version="1.0" ?>

<BIF VERSION="0.3">
<NETWORK>

<!-- Variables -->
<VARIABLE TYPE="nature">
	<NAME>CL1</NAME>
	<PROPERTY>L1 component</PROPERTY>
	<OUTCOME>on</OUTCOME>
	<OUTCOME>off</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="nature">
	<NAME>HL1</NAME>
	<PROPERTY>L1 health</PROPERTY>
	<OUTCOME>ok</OUTCOME>
	<OUTCOME>broken</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="decision">
	<NAME>DL1T1</NAME>
	<PROPERTY>Decide T1</PROPERTY>
	<OUTCOME>yes</OUTCOME>
	<OUTCOME>no</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="nature">
	<NAME>RL1T1</NAME>
	<PROPERTY>Result T1</PROPERTY>
	<OUTCOME>not done</OUTCOME>
	<OUTCOME>ok</OUTCOME>
	<OUTCOME>broken</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="utility">
	<NAME>UL1T1</NAME>
	<PROPERTY>R(L1H, DT1)</PROPERTY>
	<OUTCOME>0</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="decision">
	<NAME>DL1T2</NAME>
	<PROPERTY>Decide T2</PROPERTY>
	<OUTCOME>yes</OUTCOME>
	<OUTCOME>no</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="nature">
	<NAME>RL1T2</NAME>
	<PROPERTY>Result T2</PROPERTY>
	<OUTCOME>not done</OUTCOME>
	<OUTCOME>ok</OUTCOME>
	<OUTCOME>broken</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="utility">
	<NAME>UL1T2</NAME>
	<PROPERTY>R(RL1T1, DL1T2)</PROPERTY>
	<OUTCOME>0</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="decision">
	<NAME>DRL1</NAME>
	<PROPERTY>replace Light 1 T2</PROPERTY>
	<OUTCOME>replace</OUTCOME>
	<OUTCOME>keep</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="utility">
	<NAME>URL1</NAME>
	<PROPERTY>R(RT2, DRL1)</PROPERTY>
	<OUTCOME>0</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="nature">
	<NAME>CS1</NAME>
	<PROPERTY>S1 component</PROPERTY>
	<OUTCOME>on</OUTCOME>
	<OUTCOME>off</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="nature">
	<NAME>HS1</NAME>
	<PROPERTY>S1 health</PROPERTY>
	<OUTCOME>ok</OUTCOME>
	<OUTCOME>broken</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="decision">
	<NAME>DS1T1</NAME>
	<PROPERTY>Decide S1T1</PROPERTY>
	<OUTCOME>yes</OUTCOME>
	<OUTCOME>no</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="nature">
	<NAME>RS1T1</NAME>
	<PROPERTY>Result S1T1</PROPERTY>
	<OUTCOME>not done</OUTCOME>
	<OUTCOME>ok</OUTCOME>
	<OUTCOME>broken</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="utility">
	<NAME>US1T1</NAME>
	<PROPERTY>R(HS1, DS1T1)</PROPERTY>
	<OUTCOME>0</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="decision">
	<NAME>DRS1</NAME>
	<PROPERTY>replace switch S1</PROPERTY>
	<OUTCOME>replace</OUTCOME>
	<OUTCOME>keep</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="utility">
	<NAME>URS1</NAME>
	<PROPERTY>R(RS1T1, DRS1)</PROPERTY>
	<OUTCOME>0</OUTCOME>
</VARIABLE>

<!-- Probability distributions -->
<DEFINITION>
	<FOR>CL1</FOR><!--CL1 | HL1,CS1,-->
	<GIVEN>CS1</GIVEN>
	<GIVEN>HL1</GIVEN>
	<TABLE>0.99 0.01 0.05 0.95 0.1 0.9 0.05 0.95 </TABLE>
</DEFINITION>
<DEFINITION>
	<FOR>HL1</FOR><!--HL1 | -->
	<TABLE>0.7 0.3 </TABLE>
</DEFINITION>
<DEFINITION>
	<FOR>RL1T1</FOR><!--RL1T1 | HL1,-->
	<GIVEN>HL1</GIVEN>
	<TABLE>0.95 0.05 0 0.4 0.1 0.5 </TABLE>
</DEFINITION>
<DEFINITION>
	<FOR>UL1T1</FOR><!--UL1T1 | HL1,DL1T1,-->
	<GIVEN>HL1</GIVEN>
	<GIVEN>DL1T1</GIVEN>
	<TABLE>1 2 3 0 </TABLE>
</DEFINITION>
<DEFINITION>
	<FOR>RL1T2</FOR><!--RL1T2 | RL1T1,-->
	<GIVEN>RL1T1</GIVEN>
	<TABLE>0.95 0.05 0 0.05 0.9 0.05 0.4 0.1 0.5 </TABLE>
</DEFINITION>
<DEFINITION>
	<FOR>UL1T2</FOR><!--UL1T2 | RL1T1,DL1T2,-->
	<GIVEN>RL1T1</GIVEN>
	<GIVEN>DL1T2</GIVEN>
	<TABLE>1 2 0 3 3 2 </TABLE>
</DEFINITION>
<DEFINITION>
	<FOR>URL1</FOR><!--URL1 | DRL1,RL1T2,-->
	<GIVEN>RL1T2</GIVEN>
	<GIVEN>DRL1</GIVEN>
	<TABLE>2 4 0 3 4 4 </TABLE>
</DEFINITION>
<DEFINITION>
	<FOR>CS1</FOR><!--CS1 | HS1,-->
	<GIVEN>HS1</GIVEN>
	<TABLE>0.95 0.05 0.1 0.9 </TABLE>
</DEFINITION>
<DEFINITION>
	<FOR>HS1</FOR><!--HS1 | -->
	<TABLE>0.7 0.3 </TABLE>
</DEFINITION>
<DEFINITION>
	<FOR>RS1T1</FOR><!--RS1T1 | HS1,-->
	<GIVEN>HS1</GIVEN>
	<TABLE>0.95 0.05 0 0.4 0.1 0.5 </TABLE>
</DEFINITION>
<DEFINITION>
	<FOR>US1T1</FOR><!--US1T1 | HS1,DS1T1,-->
	<GIVEN>HS1</GIVEN>
	<GIVEN>DS1T1</GIVEN>
	<TABLE>1 2 3 0 </TABLE>
</DEFINITION>
<DEFINITION>
	<FOR>URS1</FOR><!--URS1 | DRS1,RS1T1,-->
	<GIVEN>DRS1</GIVEN>
	<GIVEN>RS1T1</GIVEN>
	<TABLE>2 0 4 4 3 4 </TABLE>
</DEFINITION>

</NETWORK>
</BIF>
