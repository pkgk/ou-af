	<OUTCOME>negative</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="decision">
	<NAME>DRMotor</NAME>
	<PROPERTY>Replace</PROPERTY>
	<OUTCOME>yes</OUTCOME>
	<OUTCOME>no</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="decision">
	<NAME>DTMotor</NAME>
	<PROPERTY>Test</PROPERTY>
	<OUTCOME>yes</OUTCOME>
	<OUTCOME>no</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="utility">
	<NAME>URMotor</NAME>
	<PROPERTY>Ureplace</PROPERTY>
	<OUTCOME>U\(H, DR\)</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="utility">
	<NAME>UTMotor</NAME>
	<PROPERTY>Utest</PROPERTY>
	<OUTCOME>U\(DT\)</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="nature">
	<NAME>Hblades</NAME>
	<PROPERTY>Health</PROPERTY>
	<OUTCOME>ok</OUTCOME>
	<OUTCOME>broken</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="nature">
	<NAME>Tblades</NAME>
	<PROPERTY>Testoutcome</PROPERTY>
	<OUTCOME>notdone</OUTCOME>
	<OUTCOME>positive</OUTCOME>
