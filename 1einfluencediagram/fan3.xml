</VARIABLE>

<VARIABLE TYPE="decision">
	<NAME>DRblades</NAME>
	<PROPERTY>Replace</PROPERTY>
	<OUTCOME>yes</OUTCOME>
	<OUTCOME>no</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="decision">
	<NAME>DTblades</NAME>
	<PROPERTY>Test</PROPERTY>
	<OUTCOME>yes</OUTCOME>
	<OUTCOME>no</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="utility">
	<NAME>URblades</NAME>
	<PROPERTY>Ureplace</PROPERTY>
	<OUTCOME>U\(H, DR\)</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="utility">
	<NAME>UTblades</NAME>
	<PROPERTY>Utest</PROPERTY>
	<OUTCOME>U\(DT\)</OUTCOME>
</VARIABLE>

<!-- Probability distributions -->
<DEFINITION>
	<FOR>M</FOR>
	<GIVEN>HMotor</GIVEN>
	<TABLE>0.8 0.2 0.7 0.3 </TABLE>
</DEFINITION>
<DEFINITION>
	<FOR>B</FOR>
	<GIVEN>M</GIVEN>
	<GIVEN>Hblades</GIVEN>
	<TABLE>0.9 0.1 0.8 0.2 0.8 0.2 0.7 0.3 </TABLE>
</DEFINITION>
<DEFINITION>
	<FOR>HMotor</FOR>
	<TABLE>0.98 0.02 </TABLE>
</DEFINITION>
<DEFINITION>
	<FOR>TMotor</FOR>
	<GIVEN>DTMotor</GIVEN>
	<GIVEN>HMotor</GIVEN>
	<TABLE>0.1 0.8 0.1 0.1 0.1 0.8 0.7 0.2 0.1 0.7 0.1 0.2 </TABLE>
</DEFINITION>
<DEFINITION>
	<FOR>DRMotor</FOR>
	<GIVEN>TMotor</GIVEN>
</DEFINITION>
<DEFINITION>
	<FOR>URMotor</FOR>
	<GIVEN>HMotor</GIVEN>
	<GIVEN>DRMotor</GIVEN>
