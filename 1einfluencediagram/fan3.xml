<?xml version="1.0" ?>

<BIF VERSION="0.3">
<NETWORK>

<!-- Variables -->
<VARIABLE TYPE="nature">
	<NAME>M</NAME>
	<PROPERTY>Motor</PROPERTY>
	<OUTCOME>ok</OUTCOME>
	<OUTCOME>broken</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="nature">
	<NAME>B</NAME>
	<PROPERTY>blades</PROPERTY>
	<OUTCOME>ok</OUTCOME>
	<OUTCOME>broken</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="nature">
	<NAME>HMotor</NAME>
	<PROPERTY>Health</PROPERTY>
	<OUTCOME>ok</OUTCOME>
	<OUTCOME>broken</OUTCOME>
</VARIABLE>

<VARIABLE TYPE="nature">
	<NAME>TMotor</NAME>
	<PROPERTY>Testoutcome</PROPERTY>
	<OUTCOME>notdone</OUTCOME>
